<div class="col-md-12", id="container">
  <div class="row">

    <div class="col-md-10 col-md-offset-1" id="middle_block">

      <div class="row">
        <div class="col-md-8 col-md-offset-2 header">
          <div class="col-md-12 logo_and_tab">
            <div id="logo">
              <a href="https://www.mint.com"><img src="https://images.mint.com/web-client/images/logos-retina/mint_logo@2x.png"></a>
            </div>

            <div id="tab">
                <div class="tab_title", id="table_title_left">
                  <a id="signupButton", class="selected"><strong>Sign up</strong></a>
                </div>
                <div class="tab_title", id="table_title_right">
                  <a id="signinButton">Log in</a>
                </div>
            </div>
          </div>
        </div>

        <div class="col-md-8 col-md-offset-2", id="form">
                <div class="col-md-7", id="content_left">
                  <h2>See all your finances in one place</h2>
                  <br />

                  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
                    <%= devise_error_messages! %>

                    <div class="text_field">
                      <div class="field">
                        <%= f.label :email, value:'Your Email' %><br \>
                        <%= f.email_field :email, autofocus: true %>
                      </div>

                      <div class="field">
                        <%= f.label :email_confirmation, value:'Confirm Email' %><br \>
                        <%= f.email_field :email_confirmation, autocomplete: "off" %>
                      </div>

                      <div class="field">
                        <%= f.label :phone, value:'Phone (recommended)' %><br \>
                        <%= f.telephone_field :phone %><br \>
                        <span class="phone_sentence_field">Standard call, messaging or data rates may apply.</span>
                      </div>

                      <div class="field">
                        <%= f.label :password, value:'Password' %><br \>
                        <%= f.password_field :password, autocomplete: "off" %>
                      </div>

                      <div class="field">
                        <%= f.label :password_confirmation, value:'Confirm Password'  %><br \>
                        <%= f.password_field :password_confirmation, autocomplete: "off" %>
                      </div>
                      <br />
                    </div>

                    <div class="button_signup">
                      <%= f.submit "SIGN UP" %>
                    </div>
                    <br />
                  <% end %>

                    <footer class="text_footer"><span class="text_span">By clicking Sign Up you agree to our <a class="hyperlink_footer" href="https://www.mint.com/terms" target="_blank">Terms of Use</a> and <a class= "hyperlink_footer", href="https://www.mint.com/privacy" target="_blank">Privacy Statement</a></span></footer>
                </div>
                <div class="col-md-4 col-md-offset-1", id="content_right">
                  <h3>Why you'll love it </h3>
                  <ul> 
                    <li>See all your accounts in one place</li>
                    <li>Find the best ways to grow your money</li>
                    <li>Stay <a href="http://www.mint.com/privacy/" id="signup-privacy">safe and secure</a></li>
                  </ul>
                </div>
        </div>  
      </div>

      <div class="row">
        <div class="col-md-8 col-md-offset-2", id="footer">
          <div class="footer_menu">
            <a id="body-footer-help" target="_blank" href="https://help.mint.com">Support</a>
            <a id="footer-safe" target="_blank" href="http://www.mint.com/terms">Terms of Use </a>
            <a id="footer-privacy" target="_blank" href="http://www.mint.com/privacy"> Privacy</a>
            <a id="footer-terms" target="_blank" href="http://about.intuit.com/legal/" class="last">Legal and Trademark Notices </a>
            <a id="about-us" target="_blank" href="http://about.intuit.com/about_intuit/">About Intuit</a>
          </div>

          <div class="footer_paragraph">
            <p class="paragraph_footer">Mint bill pay services provided by Intuit Payments Inc. under applicable <a class="hyperlink_paragraph_footer" target="_blank" href="http://www.intuit.com/legal/outbound-licenses/">licenses</a>.</p>
           </div>

          <div class="securityLinks">
            <a href="https://seal.websecurity.norton.com/getseal?at=0&sealid=0&dn=mint.intuit.com&lang=en&tpt=opaque", target="_blank"><%= image_tag("norton.png", id:"ssl-norton") %></a>
            <a href="http://www.truste.org/ivalidate.php?url=www.mint.com&amp;sealid=101", target="_blank"><%= image_tag("truste.png", id:"ssl-truste") %></a>
          </div>

          <p class="copyright">Â© 2017 Intuit, Inc.<span class="hide" id="supportNumberContainer">&nbsp;&nbsp;|&nbsp;&nbsp;<span id="supportNumber">1.800.HELP.ME</span></span>&nbsp;&nbsp;|&nbsp;&nbsp;<span id="buildNumber">ph7758.4 - spa20170809.6 </span></p>
        </div>
      </div>

    </div>

  </div>
</div>

